{
  "name": "@keyv/mysql",
  "version": "1.1.5",
  "description": "MySQL/MariaDB storage adapter for Keyv",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "test:db:start": "docker-compose -f ./test/mysql.yaml up -d",
    "test:db:stop": "docker-compose -f ./test/mysql.yaml down -v && rm -rf ./test/.mysql-data",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "xo": {
    "extends": "xo-lukechilds",
    "rules": {
      "unicorn/prefer-module": 0
    }
  },
  "ava": {
    "require": [
      "requirable"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredwray/keyv-mysql.git"
  },
  "keywords": [
    "mysql",
    "mariadb",
    "sql",
    "keyv",
    "storage",
    "adapter",
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Jared Wray <me@jaredwray.com> (http://jaredwray.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredwray/keyv-mysql/issues"
  },
  "homepage": "https://github.com/jaredwray/keyv-mysql",
  "dependencies": {
    "@keyv/sql": "1.1.3",
    "mysql2": "2.3.2"
  },
  "devDependencies": {
    "@keyv/test-suite": "*",
    "ava": "^3.15.0",
    "coveralls": "^3.0.0",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "keyv": "*",
    "nyc": "^15.1.0",
    "requirable": "^1.0.1",
    "this": "^1.0.2",
    "xo": "^0.45.0"
  }
}
