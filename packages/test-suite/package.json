{
	"name": "@keyv/test-suite",
	"version": "1.9.0",
	"description": "Test suite for Keyv API compliancy",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"scripts": {
		"build": "tsc",
		"prepare": "yarn build",
		"test": "yarn build && xo && c8 ava --serial",
		"test:ci": "yarn build && xo && ava --serial",
		"clean": "rm -rf node_modules && rm -rf ./coverage && rm -rf dist && rm -rf ./test/testdb.sqlite"
	},
	"xo": {
		"rules": {
			"unicorn/prefer-module": 0,
			"unicorn/prefer-node-protocol": 0,
			"@typescript-eslint/no-unsafe-assignment": 0,
			"@typescript-eslint/no-confusing-void-expression": 0,
			"import/extensions": 0
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredwray/keyv.git"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"test",
		"suite",
		"tests",
		"key",
		"value",
		"store",
		"cache",
		"ttl"
	],
	"author": "Jared Wray <me@jaredwray.com> (http://jaredwray.com)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jaredwray/keyv/issues"
	},
	"homepage": "https://github.com/jaredwray/keyv",
	"dependencies": {
		"bignumber.js": "^9.1.1",
		"delay": "^5.0.0",
		"json-bigint": "^1.0.0",
		"sqlite3": "^5.1.4",
		"timekeeper": "^2.2.0"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"ts-node": "^10.9.1",
		"@keyv/compress-brotli": "*",
		"@types/json-bigint": "^1.0.1",
		"@types/json-buffer": "^3.0.0",
		"ava": "^5.1.0",
		"c8": "^7.12.0",
		"keyv": "*",
		"typescript": "^4.9.5"
	},
	"ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		]
	}
}
