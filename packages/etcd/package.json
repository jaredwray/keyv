{
	"name": "@keyv/etcd",
	"version": "2.1.1",
	"description": "Etcd storage adapter for Keyv",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --clean",
		"prepublishOnly": "pnpm build",
		"test": "biome check --write && vitest run --coverage",
		"test:ci": "biome check && vitest --run --sequence.setupFiles=list --coverage",
		"clean": "rimraf ./node_modules ./coverage ./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredwray/keyv.git"
	},
	"keywords": [
		"etcd",
		"keyv",
		"storage",
		"adapter",
		"key",
		"value",
		"store",
		"cache",
		"ttl"
	],
	"author": "Jared Wray <me@jaredwray.com> (https://jaredwray.com)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jaredwray/keyv/issues"
	},
	"homepage": "https://github.com/jaredwray/keyv",
	"dependencies": {
		"etcd3": "^1.1.2"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@keyv/test-suite": "workspace:^",
		"@vitest/coverage-v8": "^3.2.4",
		"rimraf": "^6.0.1",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"keyv": "workspace:^"
	},
	"tsd": {
		"directory": "test"
	},
	"files": [
		"dist",
		"LICENSE"
	]
}
