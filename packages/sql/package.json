{
  "name": "@keyv/sql",
  "version": "1.1.3",
  "description": "Parent class for SQL based Keyv storage adapters",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "clean": "rm -rf node_modules && rm -rf .nyc_output && rm -rf coverage.lcov && rm -rf ./test/testdb.sqlite"
  },
  "xo": {
    "extends": "xo-lukechilds",
    "rules": {
      "unicorn/prefer-module": 0
    }
  },
  "ava": {
    "require": [
      "requirable"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredwray/keyv-sql.git"
  },
  "keywords": [
    "sql",
    "keyv",
    "storage",
    "adapter",
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Jared Wray <me@jaredwray.com> (http://jaredwray.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredwray/keyv-sql/issues"
  },
  "homepage": "https://github.com/jaredwray/keyv-sql",
  "dependencies": {
    "@keyv/mysql": "^1.1.5",
    "sql": "~0.78.0"
  },
  "devDependencies": {
    "@keyv/test-suite": "*",
    "ava": "^3.15.0",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "eslint-plugin-promise": "^5.1.1",
    "keyv": "*",
    "nyc": "^15.1.0",
    "pify": "^5.0.0",
    "requirable": "^1.0.1",
    "sqlite3": "^5.0.2",
    "this": "^1.0.2",
    "xo": "^0.46.4"
  }
}
