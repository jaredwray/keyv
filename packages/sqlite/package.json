{
	"name": "@keyv/sqlite",
	"version": "4.0.5",
	"description": "SQLite storage adapter for Keyv",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "rimraf ./dist && tsup src/index.ts --format cjs,esm --dts --clean",
		"prepublishOnly": "pnpm build",
		"test": "biome check --write && vitest run --coverage",
		"test:ci": "biome check && vitest --run --sequence.setupFiles=list --coverage",
		"clean": "rimraf ./node_modules ./coverage ./test/testdb.sqlite ./dist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jaredwray/keyv.git"
	},
	"keywords": [
		"sqlite",
		"sql",
		"keyv",
		"storage",
		"adapter",
		"key",
		"value",
		"store",
		"cache",
		"ttl"
	],
	"author": "Jared Wray <me@jaredwray.com> (http://jaredwray.com)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/jaredwray/keyv/issues"
	},
	"homepage": "https://github.com/jaredwray/keyv",
	"dependencies": {
		"sqlite3": "^5.1.7"
	},
	"peerDependencies": {
		"keyv": "workspace:^"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@faker-js/faker": "^9.9.0",
		"@keyv/test-suite": "workspace:^",
		"@vitest/coverage-v8": "^3.2.4",
		"rimraf": "^6.0.1",
		"tsd": "^0.33.0",
		"vitest": "^3.2.4"
	},
	"tsd": {
		"directory": "test"
	},
	"engines": {
		"node": ">= 18"
	},
	"files": [
		"dist",
		"LICENSE"
	]
}
