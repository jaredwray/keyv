{
  "name": "keyv-api-tests",
  "version": "0.1.1",
  "description": "Test suite for Keyv API compliancy",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "babel -d dist src",
    "prebuild:map": "npm run prebuild",
    "build:map": "babel --source-maps=true -d dist src",
    "pretest": "npm run build:map",
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build"
  },
  "xo": {
    "extends": "xo-lukechilds"
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukechilds/keyv-api-tests.git"
  },
  "keywords": [
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Luke Childs <lukechilds123@gmail.com> (http://lukechilds.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukechilds/keyv-api-tests/issues"
  },
  "homepage": "https://github.com/lukechilds/keyv-api-tests",
  "dependencies": {
    "delay": "^2.0.0"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "keyv": "*",
    "nyc": "^10.3.2",
    "xo": "^0.19.0"
  }
}
