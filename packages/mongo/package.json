{
  "name": "@keyv/mongo",
  "version": "1.2.1",
  "description": "MongoDB storage adapter for Keyv",
  "main": "src/index.js",
  "scripts": {
    "test": "xo && nyc ava",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "clean": "rm -rf node_modules && rm -rf .nyc_output && rm -rf coverage.lcov"
  },
  "xo": {
    "extends": "xo-lukechilds",
    "rules": {
      "unicorn/prefer-module": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/prefer-object-from-entries": 0
    }
  },
  "ava": {
    "require": [
      "requirable"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaredwray/keyv-mongo.git"
  },
  "keywords": [
    "mongo",
    "mongodb",
    "keyv",
    "storage",
    "adapter",
    "key",
    "value",
    "store",
    "cache",
    "ttl"
  ],
  "author": "Jared Wray <me@jaredwray.com> (https://jaredwray.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaredwray/keyv-mongo/issues"
  },
  "homepage": "https://github.com/jaredwray/keyv-mongo",
  "dependencies": {
    "mongojs": "^3",
    "pify": "^5.0.0"
  },
  "devDependencies": {
    "@keyv/test-suite": "*",
    "ava": "^3.15.0",
    "eslint-config-xo-lukechilds": "^1.0.0",
    "keyv": "*",
    "nyc": "^15.1.0",
    "requirable": "^1.0.1",
    "this": "^1.1.0",
    "xo": "^0.47.0"
  }
}
